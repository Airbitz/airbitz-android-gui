<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
<item>
    <shape android:shape="rectangle" >
        <corners android:radius="@dimen/calc_button_radius" />
        <gradient
            android:angle="270"
            android:endColor="#404040"
            android:gradientRadius="360"
            android:startColor="#404040"
            android:type="linear" />

        <stroke
            android:width="0.5dp"
            android:color="#969fa8" />
    </shape>
</item>
<item
    android:bottom="2dp"
    android:left="1.2dp"
    android:right="1.2dp"
    android:top="1.2dp">
    <shape android:shape="rectangle" >
        <corners android:radius="@dimen/calc_button_radius" />
        <gradient
            android:angle="270"
            android:endColor="#0189dc"
            android:startColor="#47d0e6"
            android:type="linear" />
    </shape>
</item>
</layer-list>
